{
  "name": "gha-auto-dependabot-rebase",
  "version": "1.5.48",
  "description": "Github Action to automatically request dependabot rebases",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "lint": "tsc --noEmit",
    "pretest": "npm run compile && npm run lint",
    "test": "xo && echo 'No tests yet'",
    "codegen": "graphql-codegen-esm --config codegen.yml",
    "precompile": "rm -rf dist && npm run codegen",
    "compile": "webpack-cli --config=webpack.config.cjs",
    "dependabump": "ncu -u && rm -rf package-lock.json node_modules | true && npm i",
    "prerelease": "npm run compile",
    "release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bbeesley/gha-auto-dependabot-rebase.git"
  },
  "keywords": [
    "github",
    "actions",
    "build",
    "merge",
    "publish",
    "test"
  ],
  "author": "Bill Beesley <bill@beesley.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bbeesley/gha-auto-dependabot-rebase/issues"
  },
  "homepage": "https://github.com/bbeesley/gha-auto-dependabot-rebase#readme",
  "devDependencies": {
    "@actions/cache": "^3.3.0",
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/node": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-stage-0": "^7.8.3",
    "@babel/preset-typescript": "^7.26.0",
    "@beesley/tsconfig": "^3.0.0",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/typescript": "4.1.2",
    "@graphql-codegen/typescript-document-nodes": "4.0.12",
    "@graphql-codegen/typescript-operations": "^4.4.0",
    "@graphql-codegen/typescript-resolvers": "4.4.1",
    "@octokit/graphql-schema": "^15.25.0",
    "@octokit/webhooks-definitions": "^3.67.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/jest": "^29.5.14",
    "babel-jest": "^29.7.0",
    "babel-plugin-replace-import-extension": "^1.1.4",
    "conventional-changelog-angular": "^8.0.0",
    "conventional-commit-types": "^3.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "documentation": "^14.0.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "npm-check-updates": "^17.1.11",
    "prettier": "^3.4.1",
    "semantic-release": "^24.2.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "webpack": "^5.97.0",
    "webpack-cli": "^5.1.4",
    "xo": "^0.59.3"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "releaseRules": [
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "chore",
              "scope": "deps*",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md",
            "README.md",
            "dist/**",
            "src/generated/graphql.ts"
          ]
        }
      ]
    ]
  },
  "xo": {
    "ignores": [
      "dist",
      "src/generated"
    ],
    "space": true,
    "prettier": true,
    "rules": {
      "capitalized-comments": "off",
      "import/extensions": "off",
      "func-names": [
        "error",
        "always"
      ],
      "@typescript-eslint/no-implicit-any-catch": "off"
    }
  },
  "dependencies": {
    "@octokit/core": "^6.1.2"
  }
}
